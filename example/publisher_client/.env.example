# MiniToolStream Publisher Client Environment Variables Example

# Client Configuration
CLIENT_SERVER_ADDRESS=localhost:50051
CLIENT_TIMEOUT=10s
CLIENT_DEFAULT_SUBJECT=test.default
CLIENT_DEFAULT_CONTENT_TYPE=text/plain
CLIENT_BATCH_SIZE=10

# Vault Configuration
VAULT_ENABLED=false
VAULT_ADDR=http://localhost:8200
VAULT_TOKEN=
# VAULT_TOKEN_PATH=/var/run/secrets/vault/token
# VAULT_SECRETS_PATH=minitoolstream/publisher

# Logger Configuration
LOG_LEVEL=info
LOG_FORMAT=console
